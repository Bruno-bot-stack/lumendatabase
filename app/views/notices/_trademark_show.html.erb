<section class="notice-body">
  <% if notice.body? %>
    <h5 class="title"><%= notice.customizations&.fetch('body_field_label') || 'Alleged Infringement' %></h5>
    <p class="body"><pre><%= notice.redacted(:body) %></pre></p>
  <% end %>
  <% if notice.mark_registration_number? %>
    <h5 class="title">Mark Registration Number</h5>
    <p class="mark_registration_number"><%= notice.mark_registration_number %></p>
  <% end %>
</section>

<%= render 'works', notice: notice %>
